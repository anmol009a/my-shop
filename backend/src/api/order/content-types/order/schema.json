      "unique": false
    },
    "orderid": {
      "type": "uid",
      "required": true
    },
    "paymentinfo": {
      "type": "json"
    },
    "products": {
      "type": "json"
    },
    "address": {
      "type": "text"
    },
    "name": {
      "type": "string"
    },
    "transactionid": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "completed",
        "incomplete",
        "payment pending"
      ]
    }
